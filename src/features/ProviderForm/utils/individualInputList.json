{
  "Basic Details": [
    {
      "name": "Full name",
      "Element": "INPUT",
      "props": {
        "dbName": "name",
        "required": true
      }
    },
    {
      "name": "Name of practice/business",
      "Element": "INPUT",
      "props": {
        "dbName": "business_name"
      }
    },
    {
      "name": "Email address",
      "Element": "INPUT",
      "props": {
        "dbName": "email",
        "required": true,
        "size": "half",
        "placeholder": "yourname@example.com",
        "errorMessage": "Please enter an email address in the format: yourname@example.com",
        "pattern": "^([\\w\\.\\-]+)@([\\w\\-]+)((\\.(\\w){2,3})+)$"
      }
    },
    {
      "name": "Phone number",
      "Element": "INPUT",
      "props": {
        "dbName": "phone",
        "required": true,
        "size": "half",
        "type": "number",
        "minLength": "10",
        "maxLength": "10",
        "errorMessage": "Please enter a ten-digit phone number",
        "placeholder": "(xxx) xxx-xxxx"
      }
    },
    {
      "name": "Type of care provided",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "services",
        "required": true,
        "selection": "multiple"
      }
    },
    {
      "name": "Profile photo",
      "description": "A square photo or a photo with your face centered works best (use .jpg, .jpeg, .png, .svg, .webp)",
      "Element": "PHOTO",
      "props": {
        "dbName": "profile_photo",
        "size": "half"
      }
    },
    {
      "name": "Logo",
      "description": "A square or circular file works best (use .jpg, .jpeg, .png, .svg, .webp)",
      "Element": "PHOTO",
      "props": {
        "dbName": "logo",
        "size": "half"
      }
    }
  ],
  "Contact": [
    {
      "name": "Your website",
      "Element": "INPUT",
      "props": {
        "dbName": "website",
        "placeholder": "www.example.com"
      }
    },
    {
      "name": "Work address",
      "Element": "INPUT",
      "props": {
        "dbName": "address_1",
        "size": "full",
        "placeholder": "1234 example street"
      }
    },
    {
      "name": "Work address line 2",
      "description": "apartment, floor, suite",
      "Element": "INPUT",
      "props": {
        "dbName": "address_2",
        "size": "full"
      }
    },
    {
      "name": "City",
      "Element": "INPUT",
      "props": {
        "dbName": "city",
        "required": true,
        "size": "small"
      }
    },
    {
      "name": "State",
      "Element": "INPUT",
      "props": {
        "dbName": "state",
        "required": true,
        "size": "small"
      }
    },
    {
      "name": "Zip code",
      "Element": "INPUT",
      "props": {
        "dbName": "zip",
        "required": true,
        "size": "small",
        "type": "number",
        "errorMessage": "Please enter a five-digit zip code",
        "minLength": "5",
        "maxLength": "5"
      }
    }
  ],
  "About": [
    {
      "name": "Overview of services",
      "description": "Describe the type of care you provide (500 characters max)",
      "Element": "INPUT",
      "props": {
        "dbName": "overview",
        "required": true,
        "isTextArea": true
      }
    },
    {
      "name": "Personal bio",
      "description": "Brief introduction about yourself (500 characters max)",
      "Element": "INPUT",
      "props": {
        "dbName": "bio",
        "isTextArea": true
      }
    },
    {
      "name": "Language(s) spoken",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "languages",
        "required": true,
        "selection": "multiple"
      }
    },
    {
      "name": "Pronouns",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "pronouns",
        "required": true,
        "selection": "multiple"
      }
    }
  ],
  "Professional Details": [
    {
      "name": "Payment Options",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "paymentOptions",
        "required": true,
        "selection": "multiple"
      }
    },
    {
      "name": "Certifications",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "certifications",
        "selection": "multiple"
      }
    },
    {
      "name": "Appointment Types",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "appointmentTypes",
        "selection": "multiple"
      }
    },
    {
      "name": "In practice since",
      "description": "Enter year",
      "Element": "INPUT",
      "props": {
        "dbName": "year_established"
      }
    }
  ],
  "Demographics": [
    {
      "name": "What is your race/ethnicity?",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "ethnicity",
        "selection": "multiple",
        "isDemographics": true
      }
    },
    {
      "name": "How would you describe your gender?",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "gender",
        "selection": "multiple",
        "isDemographics": true
      }
    }
  ]
}
