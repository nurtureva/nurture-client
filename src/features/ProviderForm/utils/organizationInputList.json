{
  "Basic Details": [
    {
      "name": "Organization name",
      "Element": "INPUT",
      "props": {
        "dbName": "name",
        "required": true
      }
    },
    {
      "name": "Email address",
      "Element": "INPUT",
      "props": {
        "dbName": "email",
        "required": true,
        "size": "large",
        "placeholder": "example@example.com"
      }
    },
    {
      "name": "Phone number",
      "Element": "INPUT",
      "props": {
        "dbName": "phone",
        "required": true,
        "size": "small",
        "placeholder": "(xxx) xxx-xxxx"
      }
    },
    {
      "name": "Which of the following terms best describes your organization?",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "organizationTypes",
        "required": true,
        "selection": "multiple"
      }
    },
    {
      "name": "Logo",
      "description": "A square or circular file works best (use .jpg, .png, .tif, .heic, .psd)",
      "Element": "PHOTO",
      "props": {
        "dbName": "logo"
      }
    },
    {
      "name": "Type of care provided",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "services",
        "required": true,
        "selection": "multiple"
      }
    }
  ],
  "Contact": [
    {
      "name": "Organization website",
      "Element": "INPUT",
      "props": {
        "dbName": "website",
        "placeholder": "www.example.com"
      }
    },
    {
      "name": "Address line 1",
      "Element": "INPUT",
      "props": {
        "dbName": "address_1",
        "size": "large",
        "placeholder": "1234 example street"
      }
    },
    {
      "name": "Address line 2",
      "description": "apartment, floor, suite",
      "Element": "INPUT",
      "props": {
        "dbName": "address_2",
        "size": "small"
      }
    },
    {
      "name": "City",
      "Element": "INPUT",
      "props": {
        "dbName": "city",
        "required": true,
        "size": "small"
      }
    },
    {
      "name": "State",
      "Element": "INPUT",
      "props": {
        "dbName": "state",
        "required": true,
        "size": "small"
      }
    },
    {
      "name": "Zip code",
      "Element": "INPUT",
      "props": {
        "dbName": "zip",
        "required": true,
        "size": "small"
      }
    }
  ],
  "About": [
    {
      "name": "Overview of services",
      "description": "Describe the type of care you provide (500 characters max)",
      "Element": "INPUT",
      "props": {
        "dbName": "overview",
        "required": true,
        "isTextArea": true
      }
    }
  ],
  "Professional Details": [
    {
      "name": "Payment Options",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "paymentOptions",
        "required": true,
        "selection": "multiple"
      }
    },
    {
      "name": "Appointment Types",
      "description": "Select all that apply",
      "Element": "SELECTOR",
      "props": {
        "dbName": "appointmentTypes",
        "selection": "multiple"
      }
    },
    {
      "name": "In practice since",
      "description": "Enter year",
      "Element": "INPUT",
      "props": {
        "dbName": "year_established"
      }
    }
  ]
}
